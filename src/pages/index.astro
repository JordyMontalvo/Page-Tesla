---
import Layout from '../layouts/Layout.astro';
import Landingheader from '../components/Landingheader.astro';
import Hero from '../components/Hero.astro';
import Chargehero from '../components/Chargehero.astro';
import ModelS from "../components/ModelS.astro";
import ModelThree from "../components/ModelThree.astro";
import ModelX from "../components/ModelX.astro";
import PowerWall from "../components/PowerWall.astro";
import Accessories from "../components/Accessories.astro";
---

<Layout title="Tesla el mejor coche .">
	<Landingheader />
	<main class="snap-y snap:mandatory relative w-full h-screen overflow-auto">
		<div class="snap-center">
			<Hero />
			<div class="snap-center">
				<Chargehero />
				</div>
				<div class="snap-center">
				<ModelThree />
				</div>
				<div class="snap-center">
				<ModelS />
				</div>
				<div class="snap-center">
				<ModelX />
				</div>
				<div class="snap-center">
				<PowerWall />
				</div>
				<div class="snap-center">
				<Accessories />
				</div>

	</main>
</Layout>
